#include <stdio.h>
#include "main.h"
/**
 * main - draw a straight line in the terminal 
 *
 * return: void
 */
void print_line(int n)
{
  int i;
  if (n <= 0)
    {
      _putchar("\n");
      return;
    }
  for (i = 0; i < n; i++)
    {
      _putchar("_");
    }
  int main()
  {
    print_line(10);
    return 0;
  }
}
